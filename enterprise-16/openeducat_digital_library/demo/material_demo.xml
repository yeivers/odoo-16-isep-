        <record id="material_a_new_visitor" model="op.digital.library.material">
            <field name="material_cover" type="base64"
                   file="openeducat_digital_library/static/img/visitor.png"/>
            <field name="name">A New Visitor</field>
            <field name="material_type">pdf</field>
            <field name="publish_online">True</field>
            <field name="material_edition">First Edition</field>
            <field name="material_source">Global Digital Library</field>
            <field name="isbn_code">611324891</field>
            <field name="material_data" type="base64"
                   file="openeducat_digital_library/demo/files/A_New_Visitor_compressed.pdf"/>
            <field name="author_ids"
                   eval="[(6,0,[ref('author_alisha_berger')])]"/>
            <field name="publisher_ids"
                   eval="[(6,0,[ref('publisher_alisha_berger')])]"/>
            <field name="material_tags"
                   eval="[(6,0,[ref('material_tag_kids'),ref('material_tag_visitor'),ref('material_tag_new')])]"/>
            <field name="material_description"><![CDATA[
            <div>
            <p>A New Visitor</p>
            <p>from Room to Read</p>
            <p>All the animals are excited that someone new has arrived in the forest. Who do you think it is?</p>
            </div>
            ]]></field>
        </record>


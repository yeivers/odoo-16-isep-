<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="alumni_job_post_1" model="op.job.post">
            <field name="job_post">Web developer</field>
            <field name="street">602 Suyojan Complex</field>
            <field name="street2">Near President Hotel</field>
            <field name="city">Ahmedabad</field>
            <field name="zip">69945-000</field>
            <field name="state_id" ref="base.state_in_gj"/>
            <field name="country_id" ref="base.in"/>
            <field name="salary_from">10000</field>
            <field name="salary_upto">20000</field>
            <field name="payable_at">monthly</field>
            <field name="start_date"
